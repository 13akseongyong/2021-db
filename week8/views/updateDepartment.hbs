    <tr>
        <td>Dname</td>
        <td>Dnumber</td>
        <td>Mgr_ssn</td>
        <td>Mgr_start_date</td>
    </tr>
    {{#each dept_res}}
        <form method ="post">
            <tr>
                
                <td><input type="text" name = "dname" placeholder = {{Dname}}></td>
                {{!-- dname을 원하는 값으로 수정하기 위해 input으로 수정을 하였다. --}}
                {{!-- <td><input type="text" name = "dnumber" value = {{Dnumber}}></td> --}}
                <td>{{Dnumber}}</td>
                <td>{{Mgr_ssn}}</td>
                <td>{{Mgr_start_date}}</td>
                <td><input type="submit" value="수정" formaction = "/update/department"></td>
            </tr>
        </form>
    {{/each}}
</table>






